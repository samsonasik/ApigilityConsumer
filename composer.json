{
    "name": "samsonasik/apigility-consumer",
    "type": "library",
    "description": "Apigility Client API Service Consumer",
    "keywords": [
        "apigility",
        "zf2",
        "zf3",
        "expressive",
        "client",
        "consumer",
        "http",
        "oauth"
    ],
    "homepage": "https://github.com/samsonasik/ApigilityConsumer",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Malik Ikhsan",
            "email": "samsonasik@gmail.com",
            "homepage": "http://samsonasik.wordpress.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "zendframework/zend-http": "^2.6",
        "zendframework/zend-json": "^3.1.2",
        "zendframework/zend-servicemanager": "^3.3.0",
        "zendframework/zend-stdlib": "^3.2.1"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "kahlan/kahlan": "^4.0",
        "php-coveralls/php-coveralls": "^2.0",
        "phpstan/phpstan": "^0.11.19",
        "symfony/yaml": "^3.2.7",
        "zendframework/zend-coding-standard": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "zf": {
            "module": "ApigilityConsumer",
            "config-provider": "ApigilityConsumer\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "ApigilityConsumer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApigilityConsumer\\Spec\\": "spec/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
